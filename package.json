{
  "name": "klasa-raw-events",
  "author": "QuantumlyTangled",
  "description": "Klasa RawEvent Plugin: a plugin that adds a raw events store to klasa",
  "license": "MIT",
  "homepage": "https://github.com/KlasaCommunityPlugins/raw-events#readme",
  "bugs": {
    "url": "https://github.com/KlasaCommunityPlugins/raw-events/issues"
  },
  "main": "src/index",
  "typings": "typings/index.d.ts",
  "version": "0.0.1",
  "peerDependencies": {
    "klasa": "github:dirigeants/klasa#master",
    "discord.js": "github:discordjs/discord.js#master"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KlasaCommunityPlugins/raw-events#readme"
  },
  "scripts": {
    "docs": "npx dg --source src --custom guides/.docconfig.json --output docs/docs.json --logging",
    "lint": "npx eslint --fix src && npx eslint --fix --config .eslintrcmd.json --ext md guides && npx tslint --fix 'typings/*.ts'",
    "test": "npx eslint src && npx tslint 'typings/*.ts' && npx eslint --config .eslintrcmd.json --ext md guides && npx markdownlint guides README.md"
  },
  "devDependencies": {
    "@types/node": "^10.11.4",
    "docgen": "github:dirigeants/docsgen",
    "eslint": "^5.6.1",
    "eslint-config-klasa": "github:dirigeants/klasa-lint",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "markdownlint-cli": "^0.13.0",
    "tslint": "^5.11.0",
    "typescript": "^3.1.1"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
